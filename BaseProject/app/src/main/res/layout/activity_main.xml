<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/fragmentContainer"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/imageButton1"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageButton1"
        android:src="@drawable/menu_check_circle"
        android:layout_height="40dp"
        android:layout_width="40dp"
        android:layout_margin="8dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/imageButton2"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageButton2"
        android:src="@drawable/menu_check_circle_outline"
        android:layout_height="40dp"
        android:layout_width="40dp"
        android:layout_margin="8dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toRightOf="@id/imageButton1"
        app:layout_constraintRight_toLeftOf="@id/imageButton3"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageButton3"
        android:src="@drawable/menu_done"
        android:layout_height="40dp"
        android:layout_width="40dp"
        android:layout_margin="8dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toRightOf="@id/imageButton2"
        app:layout_constraintRight_toLeftOf="@id/imageButton4"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageButton4"
        android:src="@drawable/menu_done_outline"
        android:layout_height="40dp"
        android:layout_width="40dp"
        android:layout_margin="8dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toRightOf="@id/imageButton3"
        app:layout_constraintRight_toLeftOf="@id/imageButton5"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imageButton5"
        android:src="@drawable/menu_done_all"
        android:layout_height="40dp"
        android:layout_width="40dp"
        android:layout_margin="8dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintLeft_toRightOf="@id/imageButton4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>